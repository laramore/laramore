{
    "name": "laramore/laramore",
    "description": "Describe your table fields in the model and let Laravel the rest",
    "keywords": ["ORM", "Laravel", "Model", "Migration", "Automatic", "Auto", "Fields", "Field", "Uuid", "Meta", "Table", "Database", "Validations"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": ">=5.0",
        "nesbot/carbon": ">=1.0.0",
        "symfony/http-kernel": ">=4.0.0",
        "nastuzzi-samy/reflection-namespace": "^1.0",
        "laramore/fields": "^1.0"
    },
    "authors": [
        {
            "name": "Nastuzzi Samy",
            "email": "samy@nastuzzi.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laramore\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpcs src",
            "./vendor/bin/phpmd src text phpmd.xml"
        ]
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.3",
        "phpstan/phpstan": "^0.11.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramore\\Providers\\LaramoreProvider"
            ],
            "aliases": {
                "Metas": "Laramore\\Facades\\Metas",
                "Validations": "Laramore\\Facades\\Validations"
            }
        }
    }
}
